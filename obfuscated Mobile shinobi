local function IUCLEP_fake_script()
	local script = Instance.new('LocalScript', X)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.UIGradient.Visible = true
	end)
end
coroutine.wrap(IUCLEP_fake_script)()
local function OSVSJS_fake_script()
	local script = Instance.new('LocalScript', HitBox)

	script.Parent.MouseButton1Down:connect(function()
	    game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Enjoy By chris", Text = "Mobile Hitbox"})
	local player = game.Players.LocalPlayer
		
		
		
	_G.HeadSize = 40
	
	for i,v in next, game:GetService('Players'):GetPlayers() do
	if v.Name ~= game:GetService('Players').LocalPlayer.Name then
	pcall(function()
	v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
	v.Character.HumanoidRootPart.Transparency = 0.1
	v.Character.HumanoidRootPart.BrickColor = BrickColor.new("White")
	v.Character.HumanoidRootPart.Material = "Glass"
	v.Character.HumanoidRootPart.CanCollide = false
	end)
	end
		end
		end)
end
coroutine.wrap(OSVSJS_fake_script)()
